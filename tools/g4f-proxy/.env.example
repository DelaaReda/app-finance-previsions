# g4f-proxy environment variables

# Model registry and working models
G4F_WORKING_URL=https://raw.githubusercontent.com/maruf009sultan/g4f-working/main/working/working_results.txt
G4F_WORKING_CACHE=.g4f_working.txt
G4F_FETCH_TIMEOUT=6

# Scan/testing parameters (for model discovery)
G4F_TEST_PROMPT=ping
G4F_TEST_TIMEOUT=8
G4F_TEST_PARALLEL=16
G4F_TEST_SAMPLE=0
G4F_LIVE_CACHE=.g4f_live.json
G4F_VARIANTS_PER_CAND=4

# Prefetch settings (for filtering API key gated backends)
PREFETCH_HEAD_CHARS=1200

# Optional fallbacks when live scan fails
G4F_FALLBACK_MODELS=command-r,aria

# Max prompt length (0 = unlimited)
MAX_PROMPT_CHARS=0

# Server settings
HOST=0.0.0.0
PORT=4000
